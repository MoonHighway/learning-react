{
  "name": "color-organizer",
  "version": "5.0.0",
  "description": "A sample React application",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint .",
    "build": "./node_modules/.bin/webpack --progress",
    "prestart": "npm run build",
    "start": "./node_modules/.bin/httpster -d ./dist/ -p 3000",
    "watch:test": "./node_modules/.bin/jest --watch",
    "test": "./node_modules/.bin/jest",
    "posttest": "npm run lint",
    "coverage": "open ./coverage/lcov-report/index.html",
    "dev": "open http://localhost:3000 & ./node_modules/.bin/webpack-dev-server"
  },
  "keywords": [
    "React",
    "state",
    "setState",
    "explicitly",
    "passing",
    "props"
  ],
  "author": "Alex Banks <alex@moonhighway.com> (http://www.moonhighway.com)",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer-loader": "3.2.0",
    "babel-core": "6.24.0",
    "babel-jest": "19.0.0",
    "babel-loader": "6.4.1",
    "babel-preset-latest": "6.24.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-0": "6.22.0",
    "css-loader": "0.27.3",
    "deep-freeze": "0.0.1",
    "enzyme": "2.7.1",
    "enzyme-to-json": "1.5.0",
    "eslint": "^3.18.0",
    "eslint-plugin-react": "^6.10.3",
    "jest": "19.0.2",
    "jest-css-modules": "1.1.0",
    "jest-enzyme": "2.1.2",
    "node-sass": "4.5.1",
    "optimize-css-assets-webpack-plugin": "1.3.0",
    "postcss-loader": "1.3.3",
    "react-addons-test-utils": "15.4.2",
    "sass-loader": "6.0.3",
    "style-loader": "0.14.1",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.2"
  },
  "dependencies": {
    "httpster": "1.0.3",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-icons": "2.2.3",
    "react-redux": "5.0.3",
    "redux": "3.6.0",
    "uuid": "3.0.1"
  },
  "jest": {
    "setupFiles": [
      "./__tests__/global.js"
    ],
    "modulePathIgnorePatterns": [
      "global.js"
    ],
    "verbose": true,
    "collectCoverage": true,
    "notify": true,
    "collectCoverageFrom": [
      "src/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/node_modules/jest-css-modules"
    }
  }
}
